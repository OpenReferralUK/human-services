<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="false"
      internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log" >
  <targets>
    <target xsi:type="File" name="FileLogger" layout="${longdate}|${level}|${logger}|${threadid}|${callsite}|${message}|${exception:format=tostring}" fileName="${basedir}/logs/ServiceLoaderBucks.log" />
  </targets>

  <rules>
    <logger name="*" minlevel="Debug" writeTo="FileLogger" />
  </rules>
</nlog>